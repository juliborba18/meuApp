<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns		= "http://www.w3.org/ns/widgets"
	xmlns:gap	= "http://phonegap.com/ns/1.0"
	id		= "github.willianpts.teste"
	version 	= "0.1.1">
    
        <preference name="phonegap-version" value="2.7.0" />

	<name>app mobile</name>

	<description>
		Teste de app mobile usando Phonegap
	</description>

	<author href="https://github.com/willianpts"
		email="willianpts@gmail.com">
		Willian
	</author>

	<gap:platforms>
		<gap:platform name="android" minVersion="2.1" />
		<gap:platform name="webos" />
		<gap:platform name="symbian.wrt" />
		<gap:platform name="blackberry" project="widgets"/>
	</gap:platforms>

	<icon src="app.png" gap:role="default" />

    <gap:splash src="app.png" />
	<!-- sample preference specification -->
	<!-- <preference name="autorotate" value="false" readonly="true"/> -->
</widget>
